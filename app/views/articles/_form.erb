<%= simple_form_for @article, { class: 'input-field', multipart: true } do |f| %>
  <%= f.input :image, as: :file, label: 'Photo' %>
  <br>
  <%= f.input :title, label: 'Dish name'%>
  <%= f.input :summary, label: 'Summary' %>
  <%= f.input :description, label: 'Description'%>
  <%= f.input :author, label: 'Author(s)'%>
  <br/>
  <%= f.button :submit %>
<% end %>